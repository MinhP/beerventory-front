<div class="row">
    <div class="col-md-12">
        <table class="table">
            <thead>
                <tr>
                  <th>Name</th>
                  <th>Type</th>
                  <th>Qty</th>
                  <th>UPC</th>
                  <th class="col-md-1">Actions</th>
                </tr>
            </thead>
        <% @beer.each do |beer| %>
            <tr>
                <td><%= beer[:name] %></td>
                <td><%= beer[:type] %></td>
                <td><%= beer[:qty] %></td>
                <td><%= beer[:upc] %></td>
                <td class="col-md-1">
                    <div class="nav navbar-right">
                        <a href="/edit/<%= beer[:upc] %>" class="btn btn-primary">Edit</a>
                    </div>
                </td>
            </tr>
        <% end %>
